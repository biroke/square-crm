<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

  <!-- * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *  CONTEXTE APPLICATION  * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * 
    *
    * - Vous devez definir ici l'ensemble des mapping entre les url et les services GWT
	* 
    * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * ** * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *  -->

<beans>
	<bean id="urlMapping" class="org.gwtwidgets.server.spring.GWTHandler">
		<property name="mappings">
			<map>
				<entry key="/emailsService" value-ref="emailsServiceGwt"/>
				<entry key="/constantesEmailsService" value-ref="constantesEmailsServiceGwt"/>
			</map>
		</property>				
	</bean>
</beans>