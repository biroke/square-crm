<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mappings PUBLIC "-//DOZER//DTD MAPPINGS//EN" "http://dozer.sourceforge.net/dtd/dozerbeanmapping.dtd">

<mappings>
	<configuration>
		<date-format>dd/MM/yyyy</date-format>
	</configuration>
	
	<mapping>
	    <class-a>com.square.core.model.dto.ActionDto</class-a>
		<class-b>com.square.core.model.dto.ActionCreationDto</class-b>
		<field>
			<a>statut.identifiant</a>
			<b>idStatut</b>
		</field>
		<field>
			<a>natureAction.identifiant</a>
			<b>idNatureAction</b>
		</field>
		<field>
			<a>type.identifiant</a>
			<b>idType</b>
		</field>
		<field>
			<a>objet.identifiant</a>
			<b>idObjet</b>
		</field>
		<field>
			<a>sousObjet.identifiant</a>
			<b>idSousObjet</b>
		</field>
		<field>
			<a>campagne.identifiant</a>
			<b>idCampagne</b>
		</field>
		<field>
			<a>ressource.identifiant</a>
			<b>idCommercial</b>
		</field>
		<field>
			<a>agence.identifiant</a>
			<b>idAgence</b>
		</field>
		<field>
			<a>priorite.identifiant</a>
			<b>idPriorite</b>
		</field>
	</mapping>
	
	<mapping>
		<class-a>com.square.core.model.dto.PersonneDto</class-a>
		<class-b>com.square.tarificateur.noyau.dto.personne.PersonneDto</class-b>
		<field>
			<a>identifiant</a>
			<b>eidPersonne</b>
		</field>
		<field>
			<a>civilite.identifiant</a>
			<b>eidCivilite</b>
		</field>
		<field>
			<a>infoSante.caisseRegime.identifiant</a>
			<b>infoSante.eidRegime</b>
		</field>
		<field>
			<a>infoSante.caisse.id</a>
			<b>infoSante.eidCaisse</b>
		</field>
		<field>
			<a>csp.identifiant</a>
			<b>eidCategorieSocioProf</b>
		</field>
		<field>
			<a>sitFam.identifiant</a>
			<b>eidSituationFamiliale</b>
		</field>
		<field>
			<a>statut.identifiant</a>
			<b>eidStatut</b>
		</field>
	</mapping>

	<mapping>
		<class-a>com.square.core.model.dto.EmailDto</class-a>
		<class-b>com.square.tarificateur.noyau.dto.personne.EmailDto</class-b>
		<field>
			<a>identifiant</a>
			<b>eidEmail</b>
		</field>
		<field>
			<a>idext</a>
			<b>codeAia</b>
		</field>
	</mapping>

	<mapping>
		<class-a>com.square.core.model.dto.TelephoneDto</class-a>
		<class-b>com.square.tarificateur.noyau.dto.personne.TelephoneDto</class-b>
		<field>
			<a>id</a>
			<b>eidTelephone</b>
		</field>
		<field>
			<a>idext</a>
			<b>codeAia</b>
		</field>
		<field>
			<a>numero</a>
			<b>numTelephone</b>
		</field>
	</mapping>

	<mapping>
		<class-a>com.square.core.model.dto.AdresseDto</class-a>
		<class-b>com.square.tarificateur.noyau.dto.personne.AdresseDto</class-b>
		<field>
			<a>identifiant</a>
			<b>eidAdresse</b>
		</field>
		<field>
			<a>numVoie</a>
			<b>numeroVoie</b>
		</field>
		<field>
			<a>complementAdresse</a>
			<b>residenceBatiment</b>
		</field>
		<field>
			<a>typeVoie.identifiant</a>
			<b>eidTypeVoie</b>
		</field>
		<field>
            <a>codePostalCommune.identifiant</a>
            <b>eidCodePostalCommune</b>
        </field>
		<field>
			<a>codePostalCommune.codePostal.identifiant</a>
			<b>eidCodePostal</b>
		</field>
		<field>
			<a>codePostalCommune.commune.identifiant</a>
			<b>eidCommune</b>
		</field>
		<field>
			<a>pays.identifiant</a>
			<b>eidPays</b>
		</field>
		<field>
			<a>npai</a>
			<b>topNpai</b>
		</field>
	</mapping>

	<mapping>
		<class-a>com.square.user.core.dto.UtilisateurDto</class-a>
		<class-b>com.square.core.model.dto.RessourceDto</class-b>
		<field-exclude>
			<a>id</a>
			<b>id</b>
		</field-exclude>
		<field>
			<a>id</a>
			<b>idExt</b>
		</field>
	</mapping>

	<!-- Mapping AgenceReelleDto <-> AgenceDto -->
	<mapping type="one-way">
	    <class-a>com.square.user.core.dto.AgenceReelleDto</class-a>
		<class-b>com.square.core.model.dto.AgenceDto</class-b>
		 <field-exclude> 
		     <a>id</a>
		     <b>id</b>
		 </field-exclude>
		 <field> 
		     <a>denomination</a>
		     <b>libelle</b>
		 </field>
	</mapping>
	
</mappings>